{
  "manifest_version": 2,
  "name": "SuperLibrus",
  "description": "Pomaga w obsłudze Librusa.",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [{
      "matches": ["*://*.librus.pl/nieobecnosci/wykaz_uczeszczania/*"],
      "js": ["toggleAllCheckboxes.js"]
  }],
  "developer": {
    "name": "Andrzej Wódkiewicz",
    "url": "https://github.com/akwodkiewicz/super-librus"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@example.com",
      "strict_min_version": "42.0"
    }
  }
}